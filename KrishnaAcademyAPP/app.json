        "foregroundImage": "./assets/icons/app.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.krishna.jythu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "295d8938-c7a6-49ea-872b-12ea8af7db03"
      }
    },
    "owner": "harsh10021",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/295d8938-c7a6-49ea-872b-12ea8af7db03"
    }
  }
